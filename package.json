{
    "name": "iogr-client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@material-ui/core": "^4.3.3",
        "@material-ui/icons": "^4.2.1",
        "@material-ui/styles": "^4.3.3",
        "idb": "^4.0.5",
        "material-table": "^1.59.0",
        "mobx": "^5.13.0",
        "mobx-react": "^6.1.1",
        "moment": "^2.25.3",
        "moment-timezone": "^0.5.28",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-icons": "^3.7.0",
        "react-markdown": "^4.2.2",
        "react-router-dom": "^5.0.1",
        "react-scripts": "3.0.1",
        "shards-react": "^1.0.3"
    },
    "scripts": {
        "start:local": "env-cmd -f .env.local react-scripts start",
        "start:dev": "env-cmd -f .env.development react-scripts start",
        "start:prod": "env-cmd -f .env react-scripts start",
        "build:experimental": "env-cmd -f .env.experimental react-scripts build",
        "build:dev": "env-cmd -f .env.development react-scripts build",
        "build:prod": "env-cmd -f .env react-scripts build",
        "postbuild": "purgecss --css build/static/css/*.css --content build/static/index.html build/static/js/*.js --output build/static/css",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-decorators": "^7.4.4",
        "@fullhuman/postcss-purgecss": "^2.1.0",
        "@types/node": "^12.7.12",
        "@types/react": "^16.9.6",
        "@types/react-dom": "^16.9.2",
        "awesome-typescript-loader": "^5.2.1",
        "env-cmd": "^10.0.1",
        "prettier": "^2.0.1",
        "source-map-loader": "^0.2.4",
        "typescript": "^3.6.4"
    }
}